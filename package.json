{
  "name": "a-utils",
  "version": "1.0.37",
  "main": "index.js",
  "repository": "git@github.com:SPOTEC-LTD/a-utils.git",
  "author": "jason <usochen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "dumi dev",
    "buildSvg": "svgr --template buildIconTemplate.js -d src/icons/ src/svgs",
    "build": "npm run buildSvg && node ./build/build-lib.js ",
    "release": "npm run build && release-it"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@svgr/cli": "^5.4.0",
    "@svgr/webpack": "^5.4.0",
    "dumi": "^1.0.34",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "release-it": "^14.0.4"
  },
  "dependencies": {
    "antd": "^4.6.6",
    "classnames": "^2.2.6"
  },
  "release-it": {
    "git": {
      "tag": true,
      "commitMessage": "chore: release @vant/icons ${version}",
      "addUntrackedFiles": true,
      "requireCleanWorkingDir": false
    }
  }
}
